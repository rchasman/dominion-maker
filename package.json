{
  "name": "dominion-maker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun server.ts & vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "type": "tsc --incremental --noEmit",
    "lint": "bunx oxlint && bunx eslint . && bunx stylelint 'src/**/*.css'",
    "lint:fast": "bunx oxlint",
    "lint:full": "bunx eslint . && bunx stylelint 'src/**/*.css'",
    "lint:fix": "bunx oxlint --fix && bunx eslint . --fix && bunx stylelint 'src/**/*.css' --fix",
    "pretty": "bunx prettier . --write --experimental-cli",
    "test": "bun test",
    "check:server": "bun server.ts & SERVER_PID=$! && kill $SERVER_PID",
    "check:unused": "depcheck",
    "check:circular": "dpdm --exit-code circular:1 --warning false --tree false 'src/**/*.{ts,tsx}'",
    "check:format": "bunx prettier . --check --experimental-cli",
    "check": "bun run build & bun type & bun lint & bun test & bun check:server & bun check:unused & bun check:circular & bun check:format & wait",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.0-beta.80",
    "@ai-sdk/devtools": "^0.0.1-beta.1",
    "@ai-sdk/openai": "3.0.0-beta.89",
    "@floating-ui/dom": "^1.7.4",
    "@toon-format/toon": "^2.1.0",
    "ai": "6.0.0-beta.139",
    "preact": "^10.28.0",
    "trystero": "0.22.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.3",
    "depcheck": "^1.4.7",
    "dpdm": "^3.14.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "oxlint": "^1.32.0",
    "oxlint-tsgolint": "^0.8.6",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite-bundle-analyzer": "^1.3.1",
    "vite": "^8.0.0-beta.2"
  }
}
